+package com.akarui.guren;
+
+import androidx.appcompat.app.AppCompatActivity;
+
+import android.database.sqlite.SQLiteConstraintException;
+import android.os.Bundle;
+import android.util.Log;
+import android.view.View;
+import android.widget.Toast;
+
+import com.akarui.guren.database.GurenDatabase;
+import com.akarui.guren.database.dao.UserDAO;
+import com.akarui.guren.database.entity.User;
+
+import java.util.List;
+
+public class MainActivity extends AppCompatActivity {
+
+    @Override
+    protected void onCreate(Bundle savedInstanceState) {
+        super.onCreate(savedInstanceState);
+        setContentView(R.layout.activity_main);
+    }
+
+    public void OnButtonTestClicked(View view){
+        UserDAO db = GurenDatabase.getInstance(getApplicationContext()).userDAO();
+        User user = new User();
+        user.setUsername("test");
+        user.setPassword("test");
+        User user2 = new User();
+        user2.setUsername("test2");
+        user2.setPassword("test2");
+        try {
+            db.insertAll(user, user2);
+    
+        } catch (SQLiteConstraintException constraintException){
+            Toast.makeText(getApplicationContext(), "user existed", Toast.LENGTH_SHORT);
+        }
+
+        String text = "";
+
+        List<User> users = db.loadAll();
+        
+        for (User user3: users) {
+            text += user3.toString() + "\n";
+        }
+
+        int duration = Toast.LENGTH_LONG;
+
+        Toast toast = Toast.makeText( getApplicationContext(), text, duration);
+        toast.show();
+
+        Log.d("TEST", text);
+    }
+}
